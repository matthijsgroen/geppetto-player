{
  "name": "geppetto-player",
  "version": "1.0.0-alpha.2",
  "description": "WebGL Player for Geppetto files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:matthijsgroen/geppetto-player.git",
  "author": "Matthijs Groen <matthijs.groen@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "parcel build ./src/index.ts",
    "test": "jest",
    "lint": "eslint src/",
    "presize": "yarn build",
    "size": "size-limit"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "webpack": false
    }
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.0.0-beta.2",
    "@parcel/transformer-typescript-types": "2.0.0-beta.2",
    "@size-limit/preset-small-lib": "^4.10.1",
    "@types/jest": "^26.0.21",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "parcel": "^2.0.0-beta.2",
    "prettier": "^2.2.1",
    "size-limit": "^4.10.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "delaunator": "^4.0.1"
  }
}
